<?xml version="1.0" ?>
<robot name="spot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find spot_description)/urdf/spot_macro.xacro" />

  <!-- Parameters -->
  <xacro:arg name="arm" default="false" />

  <!-- Load Spot -->
  <xacro:load_spot
    arm="$(arg arm)" />

</robot>
